{
  "name": "@metanotes/filter",
  "version": "0.1.0",
  "main": "lib/index.js",
  "license": "Apache-2.0",
  "scripts": {
    "lint": "eslint src/*.ts src/**/*.ts",
    "build": "tsc",
    "prebuild": "yarn lint",
    "build:watch": "nodemon",
    "test": "jest",
    "build:grammar": "ts-node src/save_grammar.ts"
  },
  "dependencies": {
    "chevrotain": "^7.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-typescript": "^7.10.4",
    "@types/jest": "^24.9.0",
    "@types/node": "^14.6.4",
    "jest": "^24.9.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "yarn build",
    "ext": "ts,tsx"
  }
}
