{
  "name": "metanotes-server",
  "version": "0.1.0",
  "main": "dist/index.js",
  "scripts": {
    "--build": "tsc",
    "--prestart": "npm run build",
    "start": "node --inspect=5858 -r ts-node/register ./src/index.ts $PWD/data/ 127.0.0.1:55001",
    "start:envoy": "envoy -c envoy-config.yaml",
    "start:watch": "nodemon",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src/*.ts"
  },
  "license": "Apache-2.0",
  "private": true,
  "dependencies": {
    "@grpc/grpc-js": "^1.1.7",
    "@metanotes/server-api": "0.1.0",
    "express": "^4.17.1",
    "jmespath": "^0.15.0",
    "js-yaml": "^3.14.0",
    "markdown-yaml-metadata-parser": "^2.1.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/google-protobuf": "^3.7.3",
    "@types/jmespath": "^0.15.0",
    "@types/node": "^14.6.4",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.11",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-header": "^3.1.0",
    "eslint-plugin-jest": "24.0.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-tsdoc": "^0.2.7",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "yarn start",
    "ext": "ts"
  }
}
